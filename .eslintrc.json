{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": ["off",{
            "singleQuote": true,
            "trailingComma": "es5",
            "endOfLine": "auto"
          }],
        "class-methods-use-this": "off",
        "no-console": "off",
        "no-param-reassign":"off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "next"}],
        "linebreak-style": "off"
    }
}
